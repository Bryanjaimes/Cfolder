#include <stdio.h>
#include <time.h>
#include <stdlib.h>

#define N 8
int main()
{
    srand(time(NULL));
    int i, nums[N];
    for (i=0; i<N; i++)
    {
    nums[i] = rand()%20+1;
    printf("%d ", nums[i]);
    }
    
    printf("\n");

    for (i=N-1;i>=0;i--)
    printf("%d ", nums[i]);

    return 0;
}
